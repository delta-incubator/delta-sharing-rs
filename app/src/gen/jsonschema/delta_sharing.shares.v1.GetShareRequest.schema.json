  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Get a share by name.",
  "patternProperties": {
    "^(includeSharedData)$": {
      "description": "Query for data to include in the share.",
      "type": "boolean"
    }
