{
  "$id": "delta_sharing.recipients.v1.RecipientToken.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(activation_url)$": {
      "description": "Full activation URL to retrieve the access token. It will be empty if the token is already retrieved.",
      "type": "string"
    },
    "^(created_at)$": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Time at which this recipient token was created, in epoch milliseconds."
    },
    "^(created_by)$": {
      "description": "Username of recipient token creator.",
      "type": "string"
    },
    "^(expiration_time)$": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Expiration timestamp of the token in epoch milliseconds."
    },
    "^(updated_at)$": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Time at which this recipient token was updated, in epoch milliseconds."
    },
    "^(updated_by)$": {
      "description": "Username of recipient token updater.",
      "type": "string"
    }
  },
  "properties": {
    "activationUrl": {
      "description": "Full activation URL to retrieve the access token. It will be empty if the token is already retrieved.",
      "type": "string"
    },
    "createdAt": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Time at which this recipient token was created, in epoch milliseconds."
    },
    "createdBy": {
      "description": "Username of recipient token creator.",
      "type": "string"
    },
    "expirationTime": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Expiration timestamp of the token in epoch milliseconds."
    },
    "id": {
      "description": "Unique ID of the recipient token.",
      "type": "string"
    },
    "updatedAt": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Time at which this recipient token was updated, in epoch milliseconds."
    },
    "updatedBy": {
      "description": "Username of recipient token updater.",
      "type": "string"
    }
  },
  "title": "Recipient Token",
  "type": "object"
}
