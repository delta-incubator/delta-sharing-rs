{
  "$id": "delta_sharing.credentials.v1.UpdateCredentialRequest.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Update a credential",
  "patternProperties": {
    "^(azure_managed_identity)$": {
      "$ref": "delta_sharing.credentials.v1.AzureManagedIdentity.jsonschema.json"
    },
    "^(azure_service_principal)$": {
      "$ref": "delta_sharing.credentials.v1.AzureServicePrincipal.jsonschema.json"
    },
    "^(azure_storage_key)$": {
      "$ref": "delta_sharing.credentials.v1.AzureStorageKey.jsonschema.json"
    },
    "^(new_name)$": {
      "description": "Name of credential.",
      "minLength": 3,
      "pattern": "^[a-z][0-9a-z_]*[0-9a-z]$",
      "type": "string"
    },
    "^(read_only)$": {
      "description": "Whether the credential is usable only for read operations. Only applicable when purpose is STORAGE.",
      "type": "boolean"
    },
    "^(skip_validation)$": {
      "description": "Supply true to this argument to skip validation of the updated credential.",
      "type": "boolean"
    }
  },
  "properties": {
    "azureManagedIdentity": {
      "$ref": "delta_sharing.credentials.v1.AzureManagedIdentity.jsonschema.json"
    },
    "azureServicePrincipal": {
      "$ref": "delta_sharing.credentials.v1.AzureServicePrincipal.jsonschema.json"
    },
    "azureStorageKey": {
      "$ref": "delta_sharing.credentials.v1.AzureStorageKey.jsonschema.json"
    },
    "comment": {
      "description": "Comment associated with the credential.",
      "type": "string"
    },
    "force": {
      "description": "Force an update even if there are dependent services (when purpose is SERVICE)\n or dependent external locations and external tables (when purpose is STORAGE).",
      "type": "boolean"
    },
    "name": {
      "description": "Name of credential.",
      "minLength": 3,
      "pattern": "^[a-z][0-9a-z_]*[0-9a-z]$",
      "type": "string"
    },
    "newName": {
      "description": "Name of credential.",
      "minLength": 3,
      "pattern": "^[a-z][0-9a-z_]*[0-9a-z]$",
      "type": "string"
    },
    "owner": {
      "description": "Username of current owner of credential.",
      "type": "string"
    },
    "readOnly": {
      "description": "Whether the credential is usable only for read operations. Only applicable when purpose is STORAGE.",
      "type": "boolean"
    },
    "skipValidation": {
      "description": "Supply true to this argument to skip validation of the updated credential.",
      "type": "boolean"
    }
  },
  "title": "Update Credential Request",
  "type": "object"
}
